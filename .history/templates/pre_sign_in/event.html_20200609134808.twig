{% extends 'Base.html.twig' %}
{% block meta %}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
{% endblock %}

{% block stylesheets %}
<link href="{{ asset('calendar/fullcalendar.css') }}" rel='stylesheet'/>
  <link href="{{ asset('calendar/fullcalendar.css') }}" rel='stylesheet'/>
  <link href="{{ asset('calendar/fullcalendar.print.css') }}" rel='stylesheet' media='print'/>
{% endblock %}
{% block body %}
<div id='wrap'>
<div id='calendar'></div>
<div style='clear:both'></div>
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('calendar/calendarScript.js') }}"></script>
    <script src="{{ asset('calendar/jquery-1.10.2.js') }}" type="text/javascript"></script>
    <script src="{{ asset('calendar/jquery-ui.custom.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('calendar/fullcalendar.js') }}" type="text/javascript"></script>
{% endblock %}
